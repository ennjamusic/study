<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://java.sun.com/xml/ns/javaee"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">
    <servlet>
        <servlet-name>RequestPL</servlet-name>
        <servlet-class>RequestPL</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RequestID</servlet-name>
        <servlet-class>RequestID</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MainServlet</servlet-name>
        <servlet-class>MainServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>RequestPL</servlet-name>
        <url-pattern>/RequestPL</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RequestID</servlet-name>
        <url-pattern>/RequestID</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MainServlet</servlet-name>
        <url-pattern>/main</url-pattern>
    </servlet-mapping>
</web-app>
